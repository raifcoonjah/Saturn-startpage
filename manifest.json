{
  "manifest_version": 2,
  "name": "Saturn Startpage",
  "short_name": "Saturn-start",
  "version": "6.1.0",
  "description": "Saturn Startpage is a free & open source new-tab replacement build with speed and user experience in mind.",
  "author": "mraif13",

  "icons": {
    "64": "img/icon64.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "{aa7b057d-721f-4ed8-ae73-563f767006ee}",
      "update_url": "https://raw.githubusercontent.com/mraif13/Saturn-startpage/master/updates.json"
    }
  },

  "permissions": ["storage"],
  "content_security_policy": "script-src; object-src 'self';"
}
